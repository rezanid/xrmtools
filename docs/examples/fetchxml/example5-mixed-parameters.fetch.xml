<?xml version="1.0" encoding="utf-8" ?>
<!-- Example 5: Mixed parameters - element and value-based, with and without defaults -->
<fetch distinct='false' aggregate='true'>
  <entity name="{{entityName:account}}">
    <attribute name='accountid' aggregate='count' alias='account_count' />
    <param name='filterXml'>
      <filter type='and'>
        <condition attribute='address1_city' operator='eq' value='{{cityName:Redmond}}' />
        <condition attribute='statecode' operator='eq' value='{{stateCode}}' />
      </filter>
    </param>
    <order attribute='{{orderByAttribute:name}}' descending='false' />
  </entity>
</fetch>
